<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

  <module name="gst" package="com.axelor.gst.db" />
  <entity name="BankAccount">
    <string name="bank" required="true" title="Bank Name"/>
    <string name="accountNo" required="true" title="Account No"/>
    <string name="ifsc" required="true"  title="IFSC Code"/>
    <string name="typeSelect" selection="bank.account.type.selection" default="Current" title="Bank Account Type" />
    <string name="address" multiline="true" title="Bank Address"/>
  </entity>
</domain-models>
