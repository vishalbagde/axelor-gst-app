<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

  <module name="gst" package="com.axelor.gst.db" />
  <entity name="Company">

    <string name="name" required="true"/>
    <many-to-one name="logo" ref="com.axelor.meta.db.MetaFile"/>
    <many-to-one name="contact" ref="Contact"/>
    <string name="gstin" min="15" max="15"/>
    <one-to-many name="bankDetails" ref="BankAccount" />
    <many-to-one name="address" ref="Address" ></many-to-one>

  </entity>
</domain-models>
