<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">
  <module name="gst" package="com.axelor.gst.db" />
  <entity name="Company">
    <string name="name" title="Company Name" required="true" />
    <many-to-one name="logo" title="Company Logo" ref="com.axelor.meta.db.MetaFile" />
    <one-to-many name="contactList" ref="Contact" title="Contact Details" />
    <string name="gstin" min="15" max="15" title="GSTIN NO." />
    <one-to-many name="bankAccountList" ref="BankAccount" title="Bank Account Details" />
    <many-to-one name="address" ref="Address" title="Address Details"/>
  </entity>
</domain-models>
