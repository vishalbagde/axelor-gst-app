<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

  <module name="gst" package="com.axelor.gst.db" />
  <entity name="Party">

    <string name="reference" readonly="true"/>
    <string name="name" required="true"/>
    <string name="type" required="true" selection="party.type.selection" default="Company"/>
    <boolean name="customer"/>
    <boolean name="supplier"/>
    <string name="gstin" min="15" max="15"/>
    <one-to-many name="contact" ref="Contact"/>
    <one-to-many name="address" ref="Address"/>

  </entity>
</domain-models>
